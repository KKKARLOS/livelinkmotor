<odoo>
  <data>
    <!-- Tree View -->
    <record model="ir.ui.view" id="view_service_line_tree">
      <field name="name">view.service.line.tree</field>
      <field name="model">service.line</field>
      <field name="arch" type="xml">
        <tree>
          <field name="subscription"/>
          <field name="time1"/>
          <field name="fecha_inicio"/>
          <field name="fecha_fin"/>

        </tree>
      </field>
    </record>

      <!-- Form View -->
    <record model="ir.ui.view" id="view_service_line_form">
      <field name="name">view.service.line.form</field>
      <field name="model">service.line</field>
      <field name="arch" type="xml">
        <form>
          <sheet>
            <group>
              <group>
                <field name="subscription"/>
                <field name="time1"/>
              </group>
              <group>
                <field name="fecha_inicio"/>
                <field name="fecha_fin"/>
              </group>
            </group>
          </sheet>
        </form>
      </field>
    </record>

  </data>
</odoo>