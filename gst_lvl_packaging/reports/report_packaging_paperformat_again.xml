<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="paperformat_livelink_report_packaging_again" model="report.paperformat">
        <field name="name">Report Packaging Paperformat</field>
        <field name="default" eval="False"/>
        <field name="format">custom</field>
        <field name="page_height">62</field>
        <field name="page_width">26</field>
        <field name="orientation">Landscape</field>
        <field name="margin_top">2</field>
        <field name="margin_bottom">2</field>
        <field name="margin_left">2</field>
        <field name="margin_right">2</field>
        <field name="header_line" eval="False"/>
        <field name="header_spacing">0</field>
        <field name="dpi">600</field>
    </record>

    <report id="report_label_report_packaging_again"
            model="quality.check"
            string="Product Packaging"
            name="gst_lvl_packaging.label_report_packaging_template_view_pdf_again"
            file="gst_lvl_packaging.label_report_packaging_template_view_pdf_again"
            report_type="qweb-pdf"
            paperformat="paperformat_livelink_report_packaging_again"
    />

</odoo>